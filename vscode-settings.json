{
  "breadcrumbs.enabled": true,
  "editor.detectIndentation": false,
  "editor.fontFamily": "'Operator Mono Light', Inconsolata, Consolas, 'Courier New', monospace",
  "editor.fontSize": 18,
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false
  },
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "all",
  "editor.selectionHighlight": false,
  "editor.tabSize": 2,
  "[css]": {
    "editor.tabSize": 4
  },
  "editor.wordWrap": "on",
  "emmet.triggerExpansionOnTab": true,
  "eslint.autoFixOnSave": true,
  "eslint.alwaysShowStatus": true,
  "files.defaultLanguage": "markdown",
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdownlint.config": {
    "MD033": false
  },
  "prettier.disableLanguages": ["js", "javascript", "javascriptreact"],
  "prettier.printWidth": 80,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "none",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontFamily": "'Operator Mono Light', 'Inconsolata for Powerline', 'Incolsolata', 'Consolas'",
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\wsl.exe",
  "vscode_custom_css.imports": [
    "file:///c:/Users/john.milazzo/.vscodestyles.css"
  ],
  "window.title": "${activeEditorLong}${separator}${dirty}",
  "window.titleBarStyle": "custom",
  "workbench.colorTheme": "Dark+ Material",
  "workbench.editor.highlightModifiedTabs": true,
  "files.exclude": {
    "**/.cache": true
  },
  "debug.console.fontSize": 20,
  "markdown.preview.fontSize": 20,
  "workbench.iconTheme": "material-icon-theme",
  "terminal.integrated.shell.linux": "/usr/bin/zsh",
  "workbench.panel.defaultLocation": "right",
  "files.watcherExclude": {
    "**/public/**": true
  }
}

{
  "breadcrumbs.enabled": true,
  "editor.detectIndentation": false,
  "editor.fontFamily": "'Operator Mono Light', Inconsolata, Consolas, 'Courier New', monospace",
  "editor.fontSize": 20,
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false
  },
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "all",
  "editor.selectionHighlight": false,
  "editor.tabSize": 2,
  "[css]": {
    "editor.tabSize": 4
  },
  "editor.wordWrap": "on",
  "emmet.triggerExpansionOnTab": true,
  "eslint.autoFixOnSave": true,
  "eslint.alwaysShowStatus": true,
  "files.defaultLanguage": "markdown",
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "launch": {
    "configurations": [
      {
        "name": "Launch",
        "type": "node",
        "request": "launch",
        "useWSL": true
      }
    ],
    "compounds": []
  },
  "markdownlint.config": {
    "MD033": false
  },
  "prettier.disableLanguages": ["js", "javascript", "javascriptreact"],
  "prettier.printWidth": 80,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "none",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontFamily": "'Operator Mono Light', 'Inconsolata for Powerline', 'Incolsolata', 'Consolas'",
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.cursorBlinking": true,
  "window.title": "${activeEditorLong}${separator}${dirty}",
  "window.titleBarStyle": "custom",
  "workbench.colorTheme": "Dark+ Material",
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.iconTheme": "material-icon-theme",
  "editor.cursorStyle": "underline",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.zoomLevel": 0,
  "vsonline.planFilter": "/subscriptions/a1405b8f-054d-4bc7-868b-78af7d513d44/resourceGroups/vso-rg-169e81d/providers/Microsoft.VSOnline/plans/vso-plan-eastus",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
